{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-config-prettier",
        "tslint-config-airbnb"
    ],
    "jsRules": {
        "quotemark": [
            true,
            "double"
        ]
    },
    "rules": {
        "quotes": ["error", "single"], 
        "quotemark": [
            true,
            "double"
        ],
        "max-line-length": [true, 120]
    },
    "rulesDirectory": []
}